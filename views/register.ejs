<%-include("partials/header.ejs")%>
<div class="wrapper">
    <div class="logo">
        <img src="https://www.freepnglogos.com/uploads/twitter-logo-png/twitter-bird-symbols-png-logo-0.png" alt="">
    </div>
    <div class="text-center mt-4 name">
       Register
    </div>
    <form class="p-3 mt-3" method="POST" action="/reg">
        <div class="form-field d-flex align-items-center">
            <span class="far fa-user"></span>
            <input type="text" name="userName" id="userName" placeholder="Clinic Name">
        </div>
        <div class="form-field d-flex align-items-center">
            <span class="fas fa-key"></span>
            <input type="password" name="password" id="pwd" placeholder="Password">
        </div>
        <!-- <div class="form-field d-flex align-items-center">
            <span class="fas fa-key"></span>
            <input type="text" name="hosname" id="pwd" placeholder="Clinic Name">
        </div> -->
        <button class="btn mt-3">Sign up</button>
    </form>
    <div class="text-center fs-6">
      <a href="/register">Login</a>
    </div>
</div>
<%-include("partials/footer.ejs")%>